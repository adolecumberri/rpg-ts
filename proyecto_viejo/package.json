{
  "name": "rpg-ts",
  "version": "0.9.5",
  "description": "rpg utilities in TS",
  "main": "index.js",
  "module": "index.min.mjs",
  "unpkg": "index.umd.min.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --fix --ext .ts",
    "test": "jest --coverage",
    "pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
    "postbuild": "copy package.json dist",
    "tsc": "tsc",
    "clean": "del dist",
    "build": "npm run clean && npm run lint && tsc --project tsconfig.build.json && npm run bundle:esm && npm run postbuild",
    "bundle:esm": "rollup dist/index.js --file dist/index.mjs --format esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adolecumberri/rpg-ts.git"
  },
  "author": "Adolfo Reyes Agustin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adolecumberri/rpg-ts/issues"
  },
  "homepage": "https://github.com/adolecumberri/rpg-ts#readme",
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-google": "^0.14.0",
    "jest": "^28.1.0",
    "lint": "^1.1.2",
    "rollup": "^3.10.0",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2"
  }
}
